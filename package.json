{
  "name": "@textactor/wikientity-data",
  "version": "0.2.0",
  "description": "WikiEntity data module",
  "main": "./dest/index.js",
  "types": "./dest/index.d.ts",
  "scripts": {
    "pretest": "npm run clean && npm run tsc",
    "test": "ava --no-cache --verbose --serial dest/**/*.test.js",
    "posttest": "npm run clean-test-code",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "preversion": "npm run tsc",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run tsc && npm run clean-test-code",
    "clean": "rimraf ./dest",
    "clean-test-code": "rimraf ./dest/**/*.test.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textactor/wikientity-data.git"
  },
  "keywords": [
    "wikientity",
    "data",
    "textactor"
  ],
  "author": "Dumitru Cantea",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/textactor/wikientity-data/issues"
  },
  "homepage": "https://github.com/textactor/wikientity-data#readme",
  "devDependencies": {
    "@types/node": "10.12.6",
    "ava": "^0.25.0",
    "dynamodb-local": "^0.0.22",
    "rimraf": "^2.6.2",
    "typescript": "3.1.6"
  },
  "dependencies": {
    "@textactor/domain": "^0.2.0",
    "@textactor/wikientity-domain": "^0.2.0",
    "aws-sdk": "^2.354.0",
    "dynamo-item": "^0.1.1"
  }
}
